{
    "name": "aune-io/magento2-product-grid-category-filter",
    "description": "Product grid category filter for Magento 2",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "~5.6.5|7.0.2|7.0.4|~7.0.6",
        "magento/framework": "100.1.*",
        "magento/module-catalog": "101.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1.0",
        "squizlabs/php_codesniffer": "1.5.3"
    },
    "type": "magento2-module",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Aune\\ProductGridCategoryFilter\\": ""
        }
    }
}
